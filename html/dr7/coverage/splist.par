#
# Contains a list of all Specil Stripe scans
#
# whichDatabase indicates if the run/rerun may be found in:
#  DASONLY (i.e. not in any CAS)
#  the RunsDB CAS
#  the Stripe82DB CAS
#  the DR7 CAS
#
# whichSurvey indicates if the run/rerun is from the
# Legacy SDSS-I or SDSS-II or Supernovae stripe 82 survey
# SEGUE SDSS-II survey
# A special Run, such as a M31 scan or a crossing or oblique scan
# 
# inDAS indicates 1=this run/rerun is in the DAS
#                 0=this run/rerun is CAS only
#

typedef struct {
        int run;
        int rerun;
        int stripe;
        char strip[2];
        float muStart;
        float muEnd;
        int field0;
        int nFields;
        float node;
        float incl;
        char whichDatabase[20];
        char whichSurvey[20];
        int inDAS;
} RUNINFO;


